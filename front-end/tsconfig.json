{
  "compilerOptions": {
    "outDir": "./public",
    "baseUrl": "src",
    "paths": {
      "UI": [
        "UI"
      ],
      "Domains": [
        "Domains"
      ],
      "State": [
        "State"
      ],
      "API": [
        "API"
      ]
    },
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx"
  },
  "include": [
    "./src/"
  ]
}
